
{% extends '@admin/default_frame.twig' %}

{% set menus = ['product', 'prize_list'] %}

{% block title %}当選商品管理{% endblock %}

{% block sub_title %}当選商品一覧{% endblock %}

{% block stylesheet %}{% endblock stylesheet %}

{% block javascript %}{% endblock javascript %}

{% block main %}    
    <div class="c-contentsArea__cols">
        <div class="c-contentsArea__primaryCol">
            <div class="c-primaryCol">
                <div class="card rounded border-0 mb-4">
                    <div class="card-body p-0">
                        <table class="table table-sm">
                            <thead>
                            <tr>
                                <th class="text-center">ID</th>
                                <th class="text-center">PrizeName</th>
                            </tr>
                            </thead>
                            <tbody>
                            {% for Prize in prizeList %}
                                <tr>
                                    <td class="text-center">{{ Prize.id }}</td>
                                    <td class="text-center"><a href="{{ path('prize_show_admin_edit', {id: Prize.id}) }}">{{ Prize.name }}</a></td>
                                </tr>
                            {% endfor %}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
